<h1>Edit Article</h1>

<%# <%= render 'form' %>
<%= form_with model: @article, local: true do |form| %>
<p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
</p>
<p>
    <%= form.label :text %><br>
    <%= form.text_field :text %>
</p>

<p>
    <%= form.submit %><br>
</p>

<% end %>

<%= link_to 'Back', articles_path %>